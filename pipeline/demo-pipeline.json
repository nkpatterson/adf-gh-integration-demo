{
	"name": "demo-pipeline",
	"properties": {
		"activities": [
			{
				"name": "Web1",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://jsonplaceholder.typicode.com/todos/1",
					"method": "GET",
					"headers": {
						"User-Agent": {
							"value": "@pipeline().parameters.UserAgentName",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"UserAgentName": {
				"type": "string",
				"defaultValue": "Awesome-Octocat-App"
			}
		},
		"variables": {
			"zenOutput": {
				"type": "String"
			}
		},
		"annotations": []
	}
}